{
  "build": {
    "context": "..",
    "dockerfile": "Dockerfile"
  },
  "postCreateCommand": "./.devcontainer/setup && ./scripts/setup",
  "remoteUser": "vscode",
  "mounts": [
    "source=/,target=/workspaces/dotfiles/helyi,type=bind,consistency=cached"
  ],
   "features": {
    "ghcr.io/devcontainers/features/docker-from-docker:1": {}
  },
  "forwardPorts": [
    8000,
    22112,
    22111
  ]
}
